<?xml version="1.0" encoding="UTF-8"?>
<web-app>
    <servlet>
        <servlet-name>RemoteFileSystem</servlet-name>
        <servlet-class>com.michaelssss.RemoteFileSystemServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetObject</servlet-name>
        <servlet-class>com.michaelssss.GetObjectServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GetObject</servlet-name>
        <url-pattern>/GetObject/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RemoteFileSystem</servlet-name>
        <url-pattern>/PUT</url-pattern>
    </servlet-mapping>
</web-app>